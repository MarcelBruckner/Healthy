// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Entry {
  id             String   @id @default(cuid())
  datetime       DateTime
  ort            String
  motivation     String
  speisen        String
  getraenke      String
  beschwerden    String
  stuhltyp       Int
  stuhlverhalten String
  therapie       String
  anmerkungen    String
}
